class Rectangulo:
    def __init__(self, base, altura):
        self._base = base
        self._altura = altura

    def area(self):
        return self._base * self._altura

    def perimetro(self):
        return 2 * (self._base + self._altura)

rect = Rectangulo(4, 5)
print(rect.area())
print(rect.perimetro())
